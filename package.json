{
  "name": "node-template",
  "version": "2.0.0",
  "license": "MIT",
  "main": "src/index.ts",
  "author": "Cristian Gonzalez <degonzalez996@gmail.com>",
  "repository": "https://github.com/yarmolem/node_template.git",
  "scripts": {
    "watch:ts": "swc src -w --out-dir build",
    "watch:js": "nodemon --watch \"build/**/*\" -e js ./build/index.js",
    "dev": "rimraf build && cross-env NODE_ENV=development concurrently \"npm run watch:ts\" \"npm run watch:js\"",
    "build": "swc src -d build",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "typeorm": "typeorm-ts-node-esm",
    "gen:jwt-keys": "ts-node generate_jwt_keys.ts"
  },
  "_moduleAliases": {
    "@src": "./build"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.5.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "argon2": "^0.31.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.3",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  }
}
